{
    "options": {
        "paths": {
            "root": "",
            "fonts": "fonts",
            "sprites": "",
            "styles": "styles",
            "mbtiles": "data"
        },
        "domains": [
            "localhost:8080",
            "127.0.0.1:8080"
        ],
        "formatQuality": {
            "jpeg": 80,
            "webp": 90,
            "pngQuantization": false,
            "png": 90
        },
        "maxScaleFactor": 3,
        "maxSize": 2048,
        "pbfAlias": "pbf",
        "serveAllFonts": true,
        "serveStaticMaps": true
    },
    "styles": {
        "ne-homebase-dark": {
            "style": "ne-homebase-dark.json",
            "serve_rendered": true,
            "tilejson": {
                "type": "overlay"
            }
        },
        "basic": {
            "style": "basic-v9.json",
            "serve_rendered": true,
            "tilejson": {
                "type": "overlay",
                "bounds": [-88.505, 41.339, -87.066, 42.297]
            }
        }
    },
    "data": {
        "ne-vector": {
            "mbtiles": "natural_earth.vector.mbtiles"
        },
        "chicago": {
            "mbtiles": "chicago.mbtiles"
        }
    }
}
